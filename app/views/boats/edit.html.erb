<div class="container">

<h1 class="my-4">Edit a Boat </h1>


<%= form_with model: @boat, class: "edit-form" do |f|%>
  <%= f.label :brand %>
  <%= f.text_field :brand %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.label :location %>
  <%= f.text_field :location %>
  <%= f.label :name %>
  <%= f.text_field :name %>
   <%= f.label :category %>
  <%= f.text_field :category %>
  <%= f.submit %>
<% end %>

</div>
